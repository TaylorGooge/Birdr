{{#extend "scripts"}}
    <script src="js/profile.js"></script>
{{/extend}}
{{#extend "stylesheets"}}
{{/extend}}
{{> preloader}}
{{> navbar }}
<main class="main-content" id="main-content">
    <section class="position-relative bg-white border-bottom">
        <div class="container position-relative py-9">
            <div class="row">
                <div class="col-md-6 mx-auto">
                    {{>profilebreadcrumb}}
                </div>
            </div>
            <!--/.row-->

        </div>
    </section>
    <section class="position-relative">
        <div class="container position-relative">
            <div class="overflow-hidden">
                <!--Profile info header-->
                <div class="position-relative pt-5 pb-9 pb-lg-11">
                    <div class="row">
                        <div class="col-lg-9 mx-auto">
                            <div class="pt-5 d-flex flex-column h-100">
                                {{>profilemenu}}

                                <div class="card shadow p-3 mb-3">
                                    <h5 class="mb-4">Tracking History</h5>
                                    <div class = 'container-fluid' id= 'profileContainer'>
                                        <button class ="btn btn-primary" id="button">Permanently delete selected row</button>
                                        <table class="table table-striped table-responsive" id="profile-table">
                                            <thead>
                                                <tr>
                                                    <th>
                                                        id
                                                    </th>
                                                    <th>
                                                        userId
                                                    </th>
                                                    <th>
                                                        birdId
                                                    </th>
                                                    <th>
                                                        Bird
                                                    </th>
                                                    <th>
                                                        Date
                                                    </th>
                                                </tr>
                                            </thead>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>    
</main>
{{> footer-3 }}
{{>scroller}}
